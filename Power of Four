class Solution(object):
    def isPowerOfFour(self, n):
        """
        :type n: int
        :rtype: bool
        """
        #print(n)
        if n==4:
            return True
        if n==1:
            return True
        if n%4 != 0:
            return False 
        
        self.isPowerOfFour(int(n/4))
        return True/4))
        return True
